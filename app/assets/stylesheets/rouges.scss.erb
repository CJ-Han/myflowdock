
.message-body{
<%= Rouge::Themes::Base16.mode(:light).render(scope: '.highlight') %>

  div.highlight {
    font-size: 14px;
    line-height: 18px;
    padding-bottom: 0px;
    margin-bottom: 12px;
  }
  .highlight .err{
      background-color: transparent;
  }
  pre.highlight {
      font-size: 14px;
      line-height: 18px;
      border-radius: 5px;
      padding-bottom: 10px;
      width: 90%;
  }
  p{
    font-size:16px;
  }
}



